package com.TaskHunter.project;

@Configuration
public class WebMvcConfig implements WebMvcConfigurer {
 
 @Override
 public void addCorsMappings(CorsRegistry registry) {
 registry.addMapping(“/**”)
 .allowedMethods(“*”)
 .allowedHeaders(“*”)
 .allowedOrigins(“http://192.168.1.9:3000”)
 .allowCredentials(false)
 .maxAge(-1);
 }
}