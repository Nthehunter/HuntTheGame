type AppUser {
	idAppUser: ID!,
	email: String,
	password: String,
	userName: String,
	photo: byte[],
	rol: int
}

type Query {
	appusers: [AppUser],
	appuser(idAppUser: ID): AppUser
}

type Mutation {
	createAppUser(email: String, password: String, userName: String, photo: byte[], rol: int): AppUser
}