package com.TaskHunter.project.mutation;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.TaskHunter.project.entity.models.AppUser;
import com.TaskHunter.project.entity.services.IAppUserService;



@Component
public class AppUserMutation implements com.coxautodev.graphql.tools.GraphQLMutationResolver {

	@Autowired
	private IAppUserService AppUserService;
	
	public AppUser createAppUser( String email , String password, String userName , byte[] photo , int rol ) {
		AppUser appuser = new AppUser(email, password, userName, photo , rol );
		return AppUserService.insertMutation(appuser);
	}
}